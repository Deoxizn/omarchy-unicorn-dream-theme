  "custom/update": {
    "format": "ÔÄ°",
    "exec": "omarchy-update-available",
    "on-click": "omarchy-launch-floating-terminal-with-presentation omarchy-update",
    "tooltip-format": "Omarchy update available",
    "signal": 7,
    "interval": 21600,
  },
  "idle_inhibitor": {
    "format": "{icon}",
    "format-icons": {
      "activated": "ÔÉ¥",
      "deactivated": "Ó∞ï",
    },
  },
  "custom/voxtype": {
    "exec": "omarchy-voxtype-status",
    "return-type": "json",
    "format": "{icon}",
    "format-icons": {
      "idle": "",
      "recording": "Û∞ç¨",
      "transcribing": "Û∞îü",
    },
    "tooltip": true,
    "on-click-right": "omarchy-voxtype-config",
  },
  "custom/updatespacman": {
    "format": "{}{icon}",
    "return-type": "json",
    "format-icons": {
      "has-updates": " Û∞èñ",
      "updated": "",
    },
    "exec-if": "which waybar-module-pacman-updates",
    "exec": "waybar-module-pacman-updates --no-zero-output",
  },
  "cpu": {
    "interval": 5,
    "format": "{usage}% Û∞çõ",
    "tooltip": true,
    "tooltip-format": "CPU Usage: {usage}%",
    "on-click": "omarchy-launch-or-focus-tui btop",
    "on-click-right": "alacritty"
  },
  "memory": {
    "interval": 30,
    "format": "{used:0.1f}GiB Óâ¶",
    "on-click": "omarchy-launch-or-focus-tui btop",
    "tooltip-format": "TOTAL: {total:0.1f}GiB \nSWAP: {swapUsed:0.1f}GiB ({swapPercentage}%)",
  },

  "custom/voxtype": {
    "exec": "omarchy-voxtype-status",
    "return-type": "json",
    "format": "{icon}",
    "format-icons": {
      "idle": "",
      "recording": "Û∞ç¨",
      "transcribing": "Û∞îü",
    },
    "tooltip": true,
    "on-click-right": "omarchy-voxtype-config",
    "on-click": "omarchy-voxtype-model",
  },
  "mpris": {
    "format": "{player_icon}",
    "format-paused": "<span color='grey'>{status_icon}</span>",
    "max-length": 50,
    "interval": 1,
    "player-icons": {
      "default": "Û∞Ω∞",
      "mpv": "Û∞Ω∞",
    },
    "status-icons": {
      "paused": "Û∞è§",
    },
    "ignored-players": ["firefox", "brave"],
  },
  "custom/screenrecording-indicator": {
    "on-click": "omarchy-cmd-screenrecord",
    "exec": "$OMARCHY_PATH/default/waybar/indicators/screen-recording.sh",
    "signal": 8,
    "return-type": "json",
  },
  "group/tray-expander": {
    "orientation": "inherit",
    "drawer": {
      "transition-duration": 600,
      "children-class": "tray-group-item",
    },
    "modules": ["custom/expand-icon", "memory", "cpu"],
  },
  "custom/expand-icon": {
    "format": "Û∞™¥ ",
    "tooltip": false,
  },
  "tray": {
    "icon-size": 12,
    "spacing": 5,
    "show-passive-items": true,
  },
}
